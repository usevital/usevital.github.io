---
const { type, fill } = Astro.props

function getWidth() {
  if (type === "vital") {
    return "170"
  } else if (type === "bulb") {
    return "150"
  } else if (type === "ward") {
    return "150"
  } else if (type === "ne") {
    return "90"
  } else {
    return "170"
  }
}

const width = getWidth()
